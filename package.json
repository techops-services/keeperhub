{
  "name": "ai-workflow-builder-template",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "description": "A template for building your own AI-driven workflow automation platform",
  "scripts": {
    "dev": "pnpm discover-plugins && next dev",
    "build": "tsx scripts/migrate-prod.ts && pnpm discover-plugins && next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "check": "npx ultracite@latest check",
    "fix": "npx ultracite@latest fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-chains.ts && tsx scripts/seed-tokens.ts",
    "db:seed-chains": "tsx scripts/seed-chains.ts",
    "db:seed-tokens": "tsx scripts/seed-tokens.ts",
    "db:setup": "pnpm db:migrate && pnpm db:seed",
    "discover-plugins": "tsx scripts/discover-plugins.ts",
    "create-plugin": "tsx scripts/create-plugin.ts",
    "analyze-steps": "tsx scripts/analyze-steps.ts",
    "profile-step": "tsx scripts/profile-step.ts",
    "workflow:profile": "tsx scripts/workflow-runner-profiled.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:schedule": "vitest run tests/e2e/schedule-pipeline.test.ts",
    "test:e2e:schedule:full": "FULL_E2E=true vitest run tests/e2e/schedule-pipeline.test.ts",
    "test:e2e:runner": "vitest run tests/e2e/workflow-runner.test.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.54",
    "@ai-sdk/openai": "^2.0.80",
    "@ai-sdk/provider": "^2.0.0",
    "@aws-sdk/client-sqs": "^3.948.0",
    "@getpara/ethers-v6-integration": "2.0.0-alpha.69",
    "@getpara/server-sdk": "2.0.0-alpha.69",
    "@kubernetes/client-node": "^1.4.0",
    "@linear/sdk": "^63.2.0",
    "@mendable/firecrawl-js": "^4.6.2",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.33.0",
    "@slack/web-api": "^7.12.0",
    "@solana/web3.js": "^1.98.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.8.5",
    "@vercel/sdk": "^1.17.1",
    "@vercel/speed-insights": "^1.2.0",
    "@xyflow/react": "^12.9.2",
    "ai": "^5.0.102",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.4.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "ethers": "^6.15.0",
    "import-in-the-middle": "^2.0.2",
    "jose": "^6.1.3",
    "jotai": "^2.15.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.552.0",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "openai": "^6.8.1",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.3",
    "radix-ui": "^1.4.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-resizable-panels": "^3.0.6",
    "require-in-the-middle": "^8.0.1",
    "resend": "^6.4.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
