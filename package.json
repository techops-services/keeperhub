    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.15",
    "drizzle-kit": "^0.31.6",
    "jsdom": "^27.3.0",
    "knip": "^5.70.1",
    "prettier": "^3.7.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "ultracite": "6.3.0",
    "vitest": "^4.0.15"
  }
}
